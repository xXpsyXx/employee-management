<!DOCTYPE html>
<html lang="en">

<head>
	<meta charset="UTF-8">
	<link rel="stylesheet" href="/css/feedback.css">
	<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.3/css/all.min.css">
</head>

<body>
	<input type="checkbox" id="check">
	<header>
		<label for="check">
			<i class="fas fa-bars" id="sidebar_btn"></i>
		</label>
		<div class="left_area">
			<h3> F <span>Society</span></h3>
		</div>
		<div class="right_rea">
			<a href="/logout" class="logout_btn">logout</a>
		</div>
		<table>
			<tr>

				<th>Employee-Name</th>
				<th>Email</th>
				<th>Phone no</th>
				<th>Message</th>
			</tr>
			<%emp1Details.forEach(Support=> {%>
				<tr>
					<td>
						<%= Support.name3 %>
					</td>
					<td>
						<%= Support.email3 %>
					</td>
					<td>
						<%= Support.number3 %>
					</td>
					<td>
						<%= Support.message %>
					</td>
				</tr>
				<%})%>
		</table>
		</div>
	</header>
	<div class="sidebar">
		<center>
			<img src="/images/admin.png" class="admin_image" alt="">
			<h4>
				<%= name %>
			</h4>
		</center>
		<a href="dashboard"><i class="fas fa-desktop"></i><span>Dashboard</span></a>
		<a href="userdetails"><i class="fas fa-user"> </i><span>Employee</span></a>
		<a href="registeruser"><i class="fas fa-registered"> </i><span>Registration</span></a>
		
		<a href="canteen"><i class="fas fa-utensils"></i><span>Canteen</span></a>
		<a href="Announcement"><i class="fas fa-scroll"></i><span>Announcement</span></a>
		<a href="feedback"><i class="fas fa-comment-dots"></i><span>FeedBack</span></a>
	</div>
	<div class="content"></div>
</body>

</html>